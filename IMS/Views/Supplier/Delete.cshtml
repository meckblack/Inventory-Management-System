@model IMS.Data.Objects.Entities.Supplier

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
    <h3 class="modal-title">Deleting Supplier</h3>
</div>

<div class="modal-body">
   <h3>
       Are you sure you want to delete @Html.DisplayFor(model => model.SupplierName)  ?
   </h3>
</div>
@using (Html.BeginForm("Delete", "Supplier", FormMethod.Post))
{
    @Html.AntiForgeryToken()
    <div class="modal-footer">
        <input type="submit" value="Yes" class="btn btn-danger" />
        <button class="btn btn-warning" data-dismiss="modal">No</button>
    </div>
}
