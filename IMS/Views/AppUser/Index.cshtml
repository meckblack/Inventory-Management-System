@model IEnumerable<IMS.Models.AppUser>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Role.RoleName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AppUserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AppUserEmail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AppUserPassword)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AppUserComfirmPassword)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Role.RoleName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AppUserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AppUserEmail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AppUserPassword)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AppUserComfirmPassword)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AppUserId }) |
            @Html.ActionLink("Details", "Details", new { id=item.AppUserId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.AppUserId })
        </td>
    </tr>
}

</table>
